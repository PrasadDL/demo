<apex:page Controller="myEmpRecord">

<apex:form > 

<apex:pageBlock title="Enter info">
<apex:messages />

<apex:pageBlockButtons >
 <apex:commandButton value="Submit" action="{!Submit}"/>
</apex:pageBlockButtons>

<apex:pageBlockSection >

<apex:pageBlockSectionItem >
Employee name <apex:inputtext value="{!ename}"/> 
</apex:pageBlockSectionItem>

<apex:pageBlockSectionItem >
Department name <apex:inputtext value="{!ename}"/> 
</apex:pageBlockSectionItem>

<apex:pageBlockSectionItem >
Employee Salary <apex:inputtext value="{!ename}"/> 
</apex:pageBlockSectionItem>

<apex:pageBlockSectionItem >
Gender <apex:inputtext value="{!ename}"/> 
</apex:pageBlockSectionItem>

<apex:pageBlockSectionItem >
DateofBirth <apex:inputtext value="{!ename}"/> 
</apex:pageBlockSectionItem>

<apex:pageBlockSectionItem >
VisaStatus <apex:inputtext value="{!ename}"/> 
</apex:pageBlockSectionItem>

</apex:pageBlockSection>
</apex:pageBlock>
</apex:form>



</apex:page>